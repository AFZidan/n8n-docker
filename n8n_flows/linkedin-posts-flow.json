    {
      "parameters": {
        "jsCode": "// Prepare comprehensive data for Google Sheets review\nconst items = $input.all();\nconst sourceInputs = $('Unify AI inputs').all()\nconst sheetsData = [];\n\nfor (let i = 0; i < items.length; i++) {\n  const originalData = items[i].json;\n  const generatedPost = $('Generate Posts').all()[i]?.json?.content.parts[0].text || '';\n  const suggestedHashtags = $('Export Post Hashtags').all()[i]?.json?.content.parts[0].text || '#ØªÙƒÙ†ÙˆÙ„ÙˆØ¬ÙŠØ§ #Ø¨Ø±Ù…Ø¬Ø©';\n  \n  sheetsData.push({\n    'Date Created': sourceInputs?.[i]?.json?.published_at,\n    'Source': originalData.source,\n    'Original Title': sourceInputs?.[i]?.json?.title,\n    'Original URL': sourceInputs?.[i]?.json?.url,\n    'Author': sourceInputs?.[i]?.json?.author,\n    'Generated Arabic Post': generatedPost,\n    'AI Suggested Hashtags': suggestedHashtags,\n    'Status': 'Pending Review',\n    'Approved': 'No',\n    'Published': 'No',\n    'Engagement Score': originalData.score || 0,\n    'Article Tags': Array.isArray(originalData.tags) ? originalData.tags.join(', ') : (originalData.tags || ''),\n    'Content Type': originalData.type || 'article'\n  });\n}\n\nreturn sheetsData;"
      },
